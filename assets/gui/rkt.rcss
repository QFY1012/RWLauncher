	padding-top: 8px;
	vertical-align: -18px;

	font-size: 18;
	text-align: center;
	tab-index: auto;

	background-decorator: image;
	background-image: nocolor:widgets.png 247px 0px 406px 45px;
}

input.range sliderbar:focus
{
    /*background:#77b27a;*/
    /*background-image-t: 45px 90px;*/
}

input.range sliderbar:hover
{
	background-image-t: 45px 90px;
}

input.range sliderbar:active
{
	background-image-t: 90px 135px;
}




